{
  "name": "cuttle",
  "version": "3.0.2",
  "private": true,
  "description": "The deepest card game under the sea",
  "scripts": {
    "build": "vue-cli-service build && cp ./client/public/favicon.ico ./assets",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint --no-fix",
    "debug": "node debug app.js",
    "e2e:client": "npx cypress run --config-file tests/cypress.config.js",
    "e2e:gui": "ENABLE_VUE_DEVTOOLS=true npx cypress open --config-file tests/cypress.config.js",
    "e2e:mockprod": "CUTTLE_ENV=production npm run e2e:server",
    "e2e:server": "VUE_APP_API_URL=http://localhost:1337 npx cypress run --config-file tests/cypress.config.js",
    "format:js": "./scripts/format-project-js",
    "lint:fix": "vue-cli-service lint",
    "start": "node app.js",
    "start:client": "concurrently \"ENABLE_VUE_DEVTOOLS=true vue-cli-service serve\" \"npm run start:devtools\"",
    "start:dev": "concurrently \"npm run start:server\" \"ENABLE_VUE_DEVTOOLS=true npm run start:client\" \"npm run start:devtools\"",
    "start:devtools": "vue-devtools",
    "start:mockprod": "npm run build && CUTTLE_ENV=production npm run start:server",
    "start:server": "node app.js"
  },
  "main": "app.js",
  "dependencies": {
    "connect-redis": "3.0.2",
    "dayjs": "^1.11.3",
    "lodash": "^4.17.21",
    "machinepack-passwords": "^2.3.0",
    "sails": "^1.4.3",
    "sails-disk": "^2.1.1",
    "sails-hook-orm": "^2.1.1",
    "sails-hook-sockets": "^1.5.5",
    "sails-postgresql": "^4.0.0",
    "sails.io.js": "^1.2.1",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuetify": "^2.4.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.4",
    "@vue/cli-plugin-router": "^4.5.4",
    "@vue/cli-plugin-unit-jest": "^5.0.6",
    "@vue/cli-service": "~4.5.0",
    "@vue/devtools": "^6.1.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.1.3",
    "@vue/vue2-jest": "^27.0.0-alpha.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "chai-as-promised": "^5.3.0",
    "concurrently": "^7.2.2",
    "cypress": "^9.7.0",
    "eslint": "^6.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "jest": "^27.0.5",
    "prettier": "^1.19.1",
    "rc": "^1.2.8",
    "sass": "~1.32",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "engines": {
    "node": "16.13.2"
  }
}
