{
  "name": "cuttle",
  "version": "3.0.2",
  "private": true,
  "description": "The deepest card game under the sea",
  "scripts": {
    "build": "vue-cli-service build;cp ./client/public/favicon.ico ./assets",
    "lint": "vue-cli-service lint",
    "- client": "# ------------------------",
    "- e2e": "# ---------------------------",
    "- format": "# ------------------------",
    "- lint": "# --------------------------",
    "- server": "# ------------------------",
    "debug": "node debug app.js",
    "e2e:client": "npx cypress run",
    "e2e:gui": "npx cypress open",
    "e2e:server": "npx cypress run --config baseUrl=http://localhost:1337",
    "format:js": "./scripts/format-project-js",
    "lint:fix": "vue-cli-service lint",
    "start": "node app.js",
    "start:client": "vue-cli-service serve",
    "start:server": "node app.js"
  },
  "main": "app.js",
  "dependencies": {
    "async": "2.1.4",
    "bluebird": "^3.4.6",
    "connect-redis": "3.0.2",
    "core-js": "^3.21.1",
    "ejs": "2.5.5",
    "include-all": "~0.1.6",
    "lodash": "^4.17.21",
    "machinepack-passwords": "^2.3.0",
    "sails": "^1.4.3",
    "sails-disk": "^2.1.1",
    "sails-hook-orm": "^2.1.1",
    "sails-hook-sockets": "^1.5.5",
    "sails-postgresql": "^4.0.0",
    "sails.io.js": "^1.2.1",
    "socket.io-client": "^2.3.0",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuetify": "^2.4.3",
    "vuex": "^3.5.1"
